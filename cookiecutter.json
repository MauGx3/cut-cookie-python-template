{
  "author": "Florian Maas",
  "email": "fpgmaas@gmail.com",
  "author_github_handle": "fpgmaas",
  "project_name": "cut-cookie-python",
  "project_slug": "{{cookiecutter.project_name|lower|replace('-', '_')}}",
  "project_description": "This is a template repository for Python projects that use uv for their dependency management and much more.",
  "layout": [
    "flat",
    "src"
  ],
  "include_github_actions": [
    true,
    false
  ],
  "publish_to_pypi": [
    true,
    false
  ],
  "deptry": [
    true,
    false
  ],
  "mkdocs": [
    true,
    false
  ],
  "codecov": [
    true,
    false
  ],
  "dockerfile": [
    true,
    false
  ],
  "devcontainer": [
    true,
    false
  ],
  "type_checker": [
    "mypy",
    "pyright",
    "none"
  ],
  "open_source_license": [
    "MIT license",
    "BSD license",
    "ISC license",
    "Apache Software License 2.0",
    "GNU General Public License v3",
    "Not open source"
  ]
  ,
  "__prompts__": {
    "author": "Author full name",
    "email": "Author email",
    "author_github_handle": "Author GitHub handle",
    "project_name": "Project name",
    "project_slug": "Project package/module name",
    "project_description": "Short project description",
    "layout": {
      "__prompt__": "Package layout",
      "flat": "Flat (package at repository root)",
      "src": "src/ (package under src/)"
    },
    "include_github_actions": {
      "__prompt__": "Include GitHub Actions workflows?",
      "true": "Yes",
      "false": "No"
    },
    "publish_to_pypi": {
      "__prompt__": "Enable PyPI publishing config?",
      "true": "Yes",
      "false": "No"
    },
    "deptry": {
      "__prompt__": "Include deptry configuration?",
      "true": "Yes",
      "false": "No"
    },
    "mkdocs": {
      "__prompt__": "Include MkDocs documentation?",
      "true": "Yes",
      "false": "No"
    },
    "codecov": {
      "__prompt__": "Include Codecov configuration?",
      "true": "Yes",
      "false": "No"
    },
    "dockerfile": {
      "__prompt__": "Include Dockerfile?",
      "true": "Yes",
      "false": "No"
    },
    "devcontainer": {
      "__prompt__": "Include devcontainer?",
      "true": "Yes",
      "false": "No"
    },
    "type_checker": {
      "__prompt__": "Type checker to configure",
      "mypy": "mypy",
      "pyright": "pyright",
      "none": "None"
    },
    "open_source_license": {
      "__prompt__": "Select an open source license",
      "MIT license": "MIT",
      "BSD license": "BSD",
      "ISC license": "ISC",
      "Apache Software License 2.0": "Apache-2.0",
      "GNU General Public License v3": "GPLv3",
      "Not open source": "Not open source"
    }
  }
}
